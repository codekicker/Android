<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="QuestionsList">
		<item name="android:background">#FFFFFF</item>
		<item name="android:textColor">#000000</item>
	</style>
	<style name="QuestionsList.Headline">
		<item name="android:textColor">#0071bc</item>
		<item name="android:textStyle">bold</item>
		<item name="android:padding">5sp</item>
		<item name="android:textSize">16sp</item>
	</style>
	<style name="QuestionsList.Question">
		<item name="android:paddingLeft">5sp</item>
		<item name="android:paddingBottom">5sp</item>
		<item name="android:paddingRight">5sp</item>
	</style>
	<style name="QuestionsList.TagTextView">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:textColor">#3971C2</item>
		<item name="android:background">#FDF5CE</item>
		<item name="android:layout_marginLeft">5sp</item>
		<item name="android:textSize">12sp</item>
		<item name="android:padding">2sp</item>
	</style>
	<style name="QuestionsList.Counter">
		<item name="android:background">#DDDDDD</item>
		<item name="android:paddingLeft">5sp</item>
		<item name="android:paddingRight">5sp</item>
	</style>
	<style name="QuestionsList.Counter.Number">
		<item name="android:padding">0sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:textSize">15sp</item>
		<item name="android:gravity">center_horizontal</item>
	</style>
	<style name="QuestionsList.Counter.Text">
		<item name="android:padding">0sp</item>
		<item name="android:textSize">13sp</item>
	</style>
	<style name="QuestionsList.Time">
		<item name="android:textColor">#b1aaaa</item>
		<item name="android:gravity">right|bottom</item>
		<item name="android:paddingRight">5sp</item>
	</style>
</resources>